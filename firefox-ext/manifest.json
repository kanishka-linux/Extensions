{
  "manifest_version": 2,

  "name": "kcast-ext",
  "description": "kawaii-player casting extension for Firefox",
  "version": "1.0.0",
  "applications": {"gecko":{"id":"kcast-ext@gmail.com", "strict_min_version":"45.0"}},
   "background":{
	    
		"scripts": ["background.js"]
	   },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
  "tabs",
   "activeTab",
   "contextMenus",
   "storage"
   ]
}

{
  "manifest_version": 2,

  "name": "kcast",
  "description": "kawaii-player casting extension for Chromium",
  "version": "1.0",
   "background":{
	    "persistent": false,
		"scripts": ["background.js"]
	   },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "contextMenus",
   "storage"
   ]
}
